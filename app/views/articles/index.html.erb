<div>
  <div id="header">
    <h1>LeboWikiski</h1>
    <p class="new_article"> <%= link_to "New article", new_article_path %> </p>
  </div>
  

  <div>
    <ul>
      <% @articles.each do |article| %>
        <li>
          <p><%= link_to article.title, article_path(article) %></p>
          <p><%= article.content %></p>
        </li>
      <% end %>
    </ul>
  </div>
</div>